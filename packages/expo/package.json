{
  "name": "@collie-lang/expo",
  "version": "1.1.1",
  "description": "Expo / Metro adapter for Collie templates.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./metro-transformer": {
      "types": "./dist/metro-transformer.d.ts",
      "import": "./dist/metro-transformer.js",
      "require": "./dist/metro-transformer.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepack": "pnpm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@collie-lang/compiler": "^1.1.1"
  },
  "peerDependencies": {
    "metro-react-native-babel-transformer": "*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "rimraf": "^6.0.1"
  }
}
