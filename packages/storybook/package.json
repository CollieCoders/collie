{
  "name": "@collie-lang/storybook",
  "version": "0.0.1",
  "description": "Storybook adapter for Collie templates.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "publishConfig": { "access": "public" },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(mvp) no lint configured\""
  },
  "dependencies": {
    "@collie-lang/vite": "workspace:*",
    "@collie-lang/webpack": "workspace:*"
  },
  "devDependencies": {
    "@storybook/types": "^8.2.0",
    "typescript": "^5.7.2",
    "tsup": "^8.3.5",
    "rimraf": "^6.0.1",
    "vite": "^7.3.0",
    "webpack": "^5.94.0"
  }
}
