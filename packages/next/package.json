{
  "name": "@collie-lang/next",
  "version": "1.0.3",
  "description": "Next.js plugin for Collie template language",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(mvp) no lint configured\"",
    "test": "vitest run"
  },
  "keywords": [
    "collie",
    "nextjs",
    "plugin"
  ],
  "license": "MIT",
  "dependencies": {
    "@collie-lang/compiler": "workspace:*",
    "@collie-lang/webpack": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "next": "^14.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
