{
  "name": "@collie-lang/cli",
  "version": "1.3.2",
  "description": "Collie CLI.",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "collie": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepack": "pnpm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "echo \"(mvp) no lint configured\""
  },
  "dependencies": {
    "@collie-lang/compiler": "^1.3.2",
    "@collie-lang/config": "^1.3.2",
    "@collie-lang/vite": "^1.3.2",
    "chokidar": "^3.6.0",
    "diff": "^5.2.0",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/diff": "^5.2.3",
    "@types/prompts": "^2.4.9",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
